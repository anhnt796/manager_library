    = User.human_attribute_name :name
  .col-lg-3
    = Book.human_attribute_name :title
  .col-lg-1
    = Copy.human_attribute_name :id
  .col-lg-6
    = RegisteredCopy.human_attribute_name :registered_date
  %br
  %br
  .container
    .row
      = render partial: "book_confirmation", collection: @book_confirmations
