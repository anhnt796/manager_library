= provide :title, t(".page_title")
%h1= t ".page_title"
%div{class: "container pagination justify-content-center"}
  = paginate @registered_copies, theme: "twitter-bootstrap-4",
    pagination_class: "pagination-sm"

%div{class: "rh rh-100 section-booking"}
  .col-lg-3
    = Book.human_attribute_name :title
  .col-lg-2
    = RegisteredCopy.human_attribute_name :registered_date
  .col-lg-1
    = RegisteredCopy.human_attribute_name :borrowed
  %br
  %br
  .container
    .row
      = render @registered_copies
